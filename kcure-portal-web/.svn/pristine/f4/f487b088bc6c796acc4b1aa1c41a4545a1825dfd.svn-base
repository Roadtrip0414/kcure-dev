@charset "UTF-8";
:root{
	/* 컬러 */
	--m_d: #02105F; /* 메인컬러 블루 다크 */
	--m_c: #2E4DC6; /* 메인컬러 블루 */
	--t_c: #707070; /* 서브 텍스트 컬러 */
	--l_d: #AAAAAA; /* 라인컬러 다크 */
	--l_c: #DDDDDD; /* 라인컬러  */
	--g_c: #333333; /* 다크그레이 */
}
*{
	margin: 0;
	padding: 0;
	letter-spacing: -0.5px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	word-break: keep-all;
	font-size: 16px;
	color: #111111;
	font-weight: 400;
	font-family: 'Noto Sans KR', sans-serif;
	-webkit-text-size-adjust: none;
	line-height: 1.2;
}
*:after, *:before{
	box-sizing: border-box;
	font-size: inherit;
	color: inherit;
	font-family: inherit;
	line-height: inherit;
	font-weight: inherit;
}
*[onclick]{
	cursor: pointer;
}
*[onclick]:hover{
	text-decoration: underline;
	text-underline-position: under;
}
html{
	height: 100%;
}
body{
	height: 100%;
	font-size: inherit;
	color: inherit;
	font-weight: inherit;
	min-width: 1260px;
	overflow: auto;
}
body.on{
	overflow: hidden;
}
div, span, strong, p, h1, h2, h3, h4, h5, h6, pre{
	font-size: inherit;
	color: inherit;
	font-family: inherit;
	line-height: inherit;
	font-weight: inherit;
	text-align: inherit;
}
ul{
	color: inherit;
	font-size: inherit;
	font-weight: inherit;
	text-align: inherit;
	clear: both;
}
li {
	color: inherit;
	list-style: none;
	color: inherit;
	font-size: inherit;
	font-weight: inherit;
	line-height: inherit;
	text-align: inherit;
}
img {
	border: 0;
	text-align: inherit;
	font-size: 0;
	/* 이미지 랜더링 흐림방지 */
	image-rendering:-webkit-optimize-contrast;
	transform:translateZ(0);
	backface-visibility:hidden;
}
a {
	text-decoration: none;
	font-size: inherit;
	color: inherit;
	font-weight: inherit;
	text-align: inherit;
	line-height: inherit;
	display: block;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	-webkit-tap-highlight-color: transparent;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	transition: inherit;
	cursor: pointer;
/*	-ms-user-select: none;
	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	user-select: none;*/
}
a:hover{
	text-decoration: underline;
	text-decoration-thickness: 1px;
	text-underline-position: under;
}

.pass_lost_wrap input,
.login_wrap input,
input.user_int{
	background-color: white;
	font-size: inherit;
	color: inherit;
	line-height: inherit;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	height: 50px;
	padding: 0 16px;
	width: 100%;
	transition: all 0.3s;
	border: 1px solid #DDDDDD;
	border-radius: 10px;
}
select.user_sel{
	font-family: inherit;
	background: url('../images/admin_select.svg') no-repeat right center;
	background-color: white;
	background-size: 25px;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	padding: 0px 16px;
	padding-right: 44px;
	height: 50px;
	font-size: 16px;
	color: #4d4d4d;
	width: 100%;
	transition: all 0.3s;
	border: 1px solid #DDDDDD;
	border-radius: 10px;
}
select.user_sel[disabled]{
	background-image: none;
}
input[type=number]::-webkit-outer-spin-button,
input[type=number]::-webkit-inner-spin-button {
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	margin: 0;
}
input[type=number] {
	-moz-appearance: textfield;
}
select::-ms-expand {
	display: none;
}
textarea.user_text{
	width: 100%;
	resize: none;
	line-height: 1.5;
	padding: 16px;
	background-color: white;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	border: 1px solid #DDDDDD;
	border-radius: 10px;
}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
	color: #707070;
	font-size: inherit;
}
.login_wrap input:focus,
input.user_int:focus,
select.user_sel:focus,
textarea.user_text:focus{
	border-color: var(--m_c);
	outline: none;
}
.pass_lost_wrap input[disabled],
.login_wrap input[disabled],
input.user_int[disabled],
select.user_sel[disabled],
textarea.user_text[disabled]{
	background-color: #F4F4F4;
	border-color: #F4F4F4;
}

