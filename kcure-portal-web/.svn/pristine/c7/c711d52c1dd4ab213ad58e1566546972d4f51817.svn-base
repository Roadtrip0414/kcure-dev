package kcure.portal.cmn.api.dsz.service.impl;

import java.util.List;

public class ResvItemVO {
	
	public String reserveId; //예약ID
	public int seatNo; //예약 자리 번호
	
	public List<ResvDateVO> dates;
	
	

	public String getReserveId() {
		return reserveId;
	}

	public void setReserveId(String reserveId) {
		this.reserveId = reserveId;
	}

	public int getSeatNo() {
		return seatNo;
	}

	public void setSeatNo(int seatNo) {
		this.seatNo = seatNo;
	}

	public List<ResvDateVO> getDates() {
		return dates;
	}

	public void setDates(List<ResvDateVO> dates) {
		this.dates = dates;
	} 
	
	

}
