/* 비밀번호 표시 */
.see_int{
	position: relative;
}
.see_int a{
	position: absolute;
	right: 15px;
	top: 50%;
	transform: translateY(-50%);
	background: url(../images/see_icon.svg) center/cover no-repeat;
	width: 18px;
	height: 13px;
}
.see_int a.on{
	background-image: url(../images/see_icon_02.svg);
}

/* 에러 텍스트 */
.error_txt{
	color: #FF0000;
	padding-top: 10px;
}

.wrap{
	position: relative;
	font-size: inherit;
	color: inherit;
	min-height: calc(100% - 158px);
	/*padding-bottom: 60px;*/
}
.con{
	width: 1260px;
	/*padding: 40px 30px;*/
	padding: 50px 30px;
	padding-bottom: 80px;
	margin: 0 auto;
}
.con.on{
	padding-bottom: 140px !important;
}
.wide_con{
	width: 100%;
	padding: 100px 30px;
}
.of_hi {
	overflow: hidden;
}
.bk_hide{
	background-color: rgba(0,0,0,0.6);
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	z-index: 10000;
	display: none;
}
.bk_hide.on{
	display: block;
}

/* 헤더 */
header{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	min-width: 1260px;
	height: 120px;
	padding-top: 40px;
	z-index: 100000;
	transition: transform 0.5s;
	border-bottom: 1px solid #DDDDDD;
}
header:hover{
	background: #fff;
}
header.bg{
	background-color: white;
}
header.on{
	transform: translateY(-100%);
	position: fixed;
	transition: 0s;
}
header img{
	display: block;
}
header .top{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	background-color: #F4F4F4;
	height: 40px;
	display: flex;
	align-items: center;
	justify-content: flex-end;
	grid-gap: 30px;
	padding: 0 100px;
	font-size: 14px;
	line-height: 1;
}
header .top .body_size{
	display: flex;
	align-items: center;
	grid-gap: 10px;
}
header .top .body_size a{
	width: 15px;
	height: 15px;
	background-position: center;
	background-size: cover;
	background-repeat: no-repeat;
}
header .top .body_size a.up{
	background-image: url('../images/body_size_plus_icon.svg');
}
header .top .body_size a.down{
	background-image: url('../images/body_size_minus_icon.svg');
}
header .logo{
	bottom: 12px;
	left: 100px;
	width: 140px;
	height: 56px;
	background: url('../images/logo_b.svg')no-repeat;
	background-position: left bottom;
	background-size: contain;
}
header .middle{
	display: flex;
	grid-gap: 20px;
	padding: 0 100px;
}
header .middle .left{
	flex: 1 1 0;
	display: flex;
	align-items: center;
}
header .middle .right{
	flex: 1 1 0;
	display: flex;
	justify-content: flex-end;
}
header nav{
	flex: 3 1 0;
}
header nav > ul{
	display: flex;
	align-items: center;
	justify-content: center;
	grid-gap: 60px;
}
header nav > ul > li.gnb > a{
	position: relative;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 18px;
	font-weight: 700;
	color: black;
	height: 80px;
	text-decoration: none;
	white-space: nowrap;
}
header nav > ul > li.gnb > a:hover,
header nav > ul > li.gnb > a.on{
	color: var(--m_c);
}
header nav > ul > li.gnb > a:hover:before,
header nav > ul > li.gnb > a.on:before{
	content: "";
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	border-bottom: 2px solid var(--m_c);
}
header nav > ul > li.gnb .lnb_wrap{
	position: absolute;
	left: 0;
	right: 0;
	display: none;
	padding: 30px 0px;
	background-color: white;
}
header nav > ul > li.gnb .lnb_wrap.active{
	display: block;
}
header nav > ul > li.gnb .lnb_wrap .item{
	width: 1260px;
	padding: 0 30px;
	margin: 0 auto;
	display: flex;
}
header nav > ul > li.gnb .lnb_wrap .item ul{
	position: relative;
	min-width: 200px;
	margin-right: 40px;
}
header nav > ul > li.gnb .lnb_wrap .item ul:last-child{
	margin: 0;
}
header nav > ul > li.gnb .lnb_wrap .item ul:not(:last-child):before{
	content: "";
	position: absolute;
	top: 0;
	right: -20px;
	height: 100%;
	border-right: 1px solid #DDDDDD;
}
header nav > ul > li.gnb .lnb_wrap .item ul li{
	display: flex;
	flex-direction: column;
	grid-gap: 22px;
	font-size: 14px;
	color: #707070;
}
header nav > ul > li.gnb .lnb_wrap .item ul li a:first-child{
	font-weight: 700;
	color: #111111;
}
header nav > ul > li.gnb li.search{
	position: relative;
}
header nav > ul > li.gnb li.search input{
	position: relative;
	width: 200px;
	height: 40px;
	border-radius: 20px;
	padding-left: 20px;
	padding-right: 50px;
	border-radius: 20px;
	border: 1px solid var(--m_c);
}
header nav > ul > li.gnb li.search input:focus{
	outline: none;
}
header nav > ul > li.gnb li.search input + a.btn{
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 16px;
}
header nav > ul > li.gnb li.search input + a.btn img{
	width: 18px;
}
header .user{
	position: relative;
	display: flex;
	grid-gap: 10px;
	align-items: center;
	justify-content: flex-end;
	padding-right: 23px;
	cursor: pointer;
}
header .user:after{
	content: "";
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 0;
	width: 13px;
	height: 8px;
	background: url('../images/select_d_c_icon.svg')no-repeat;
	background-position: right center;
	background-size: contain;
	transition: all 0.3s;
}
header .user.on:after{
	transform: translateY(-50%) rotate(180deg);
}
header .user .mark{
	position: relative;
	display: flex;
	align-items: center;
	justify-content: center;
	width: 40px;
	height: 40px;
	border: 1px solid #34BA02;
	border-radius: 50%;
	font-size: 11px;
	font-weight: bold;
	color:  #34BA02;
}
header .user .mark .count,
header .user .alarm .count,
header .user .name .count{
	position: absolute;
	top: -7px;
	right: -7px;
	font-size: 10px;
	background-color: #FF0000;
	border-radius: 50%;
	transform: scale(0.9);
	color: white;
	width: 19px;
	height: 19px;
	line-height: 18px;
	display: flex;
	justify-content: center;
}
header .user .name{
	white-space: nowrap;
	position: relative;
}
header .user .drop{
	display: none;
	position: absolute;
	top: calc(100% - 20px);
	left: 0;
	flex-direction: column;
	padding-top: 20px;
	width: 100%;
}
header .user .drop .item{
	background-color: white;
	box-shadow: 0px 0px 10px rgb(0 0 0 / 15%);
	border-radius: 5px;
}
header .user .drop ul{
	display: flex;
	flex-direction: column;
	grid-gap: 16px;
	padding: 20px;
}
header .user .drop ul:first-child{
	border-bottom: 1px solid #DDDDDD;
}
header .user .drop ul li a{
	color: #777777;
	position: relative;
	width: fit-content;
}
header .user .drop ul li a:hover{
	font-weight: 700;
	color: var(--m_c);
}
header .user .drop ul li a.alarm{
	font-weight: 700;
	color: #111111;
	padding-right: 12px;
}
.header_top{
	position: absolute;
	top: 226px;
	left: 50%;
	transform: translateX(-50%);
	z-index: 10;
	width: 1260px;
	padding: 0px 30px;
	margin: 0 auto;
	text-align: right;
}
.header_top .location_wrap{
	display: inline-flex;
	align-items: center;
	grid-gap: 8px;
	font-size: 12px;
	font-weight: 500;
	color: #111111;
}
.header_top .location_wrap a:first-child:before{
	content: "";
	background: url('../images/l_home_icon.svg')no-repeat;
	background-size: cover;
	background-position: center;
	width: 10px;
	height: 10px;
	display: inline-flex;
	margin-right: 4px;
}
.header_top .location_wrap a:not(:last-child){
	position: relative;
	color: #848484;
	font-weight: 400;
	padding-right: 16px;
}
.header_top .location_wrap a:not(:last-child):after{
	content: ">";
	position: absolute;
	top: 0;
	right: 0;
	width: 7px;
	height: 20px;
}
.header_top h1.title{
	display: none;
}

/* 푸터 */
footer{
	border-top: 1px solid #C0C0C0;
	background-color: #eee;
}
footer .con{
	display: flex;
	flex-direction: column;
	justify-content: center;
	grid-gap: 25px;
	padding: 0;
	height: 230px;
}
footer .info{
	display: flex;
	grid-gap: 64px;
	justify-content: space-between;
	width: 100%;
}
footer .info img{
	width: 144px;
}
footer .info .item{
	display: flex;
	grid-gap: 64px;
}
footer .info .item .list{
	display: flex;
	flex-direction: column;
	grid-gap: 10px;
}
footer .info .item .link{
	position: relative;
	display: flex;
	grid-gap: 30px;
	font-weight: 500;
	font-size: 14px;
	color: #595959;
}
footer .info .item .link a:nth-child(2){
	font-weight: 700;
	color: black;
}
footer .info .item .link a{
	position: relative;
}
footer .info .item .link a:not(:last-child):after{
	content: "";
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: -16px;
	border-right: 1px solid #C0C0C0;
	height: 10px;
}
footer .info .item p{
	color: #595959;
	font-size: 12px;
	line-height: 1.4;
}
footer .drop_sel{
	position: relative;
	display: flex;
	align-items: center;
	width: 196px;
	height: 32px;
	font-size: 12px;
	font-weight: 500;
	color: #4D4D4D;
	border: 1px solid #C7C7C7;
	border-radius: 2px;
	padding-left: 15px;
	cursor: pointer;
	background-color: white;
}
footer .drop_sel:after{
	content: "";
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 15px;
	background: url('../images/drop_sel_icon.svg')no-repeat;
	background-position: center;
	background-size: cover;
	width: 7px;
	height: 4px;
}
footer .drop_sel .drop_box{
	position: absolute;
	left: 0;
	bottom: 100%;
	width: 100%;
	padding-bottom: 10px;
	display: none;
}
footer .drop_sel .drop_box ul{
	border: 1px solid #C7C7C7;
	overflow: hidden;
	border-radius: 2px;
