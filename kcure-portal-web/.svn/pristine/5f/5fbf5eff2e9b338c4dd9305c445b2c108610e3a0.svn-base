@charset "UTF-8";

/* 희원씨 작업 공간 */

/* 데이터소개 공통 */
.lib_top .con{
	padding-bottom: 100px;
}
.lib_top .title{
	font-size: 50px;
	font-weight: 700;
	color: var(--m_c);
	text-align: center;
	margin-bottom: 135px;
	position: relative;
}
.lib_top .title:after{
	content: '';
	position: absolute;
	bottom: calc(0px - 158px);
	left: 50%;
	transform: translateY(-50%);
	border-left: 1px solid var(--m_c);
	height: 80px;
}
.lib_top .item{
	padding: 0 150px;
	position: relative;
}
.lib_top .item ul{
	display: flex;
	flex-direction: column;
	gap: 20px;
}
.lib_top .item ul li{
	box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
	border-radius: 44px;
	padding: 18px 50px;
	text-align: center;
	font-size: 18px;
	line-height: 1.4;
}
.lib_top .btn_inn{
	padding-top: 20px;
	text-align: center;
}
.lib_top .btn_inn a{
	border: 1px solid #707070;
	border-radius: 10px;
	display: inline-block;
	padding: 12px 40px 12px 22px;
	color: #707070;
	font-weight: 500;
	position: relative;
}
.lib_top .btn_inn a:after{
	content: '';
	position: absolute;
	right: 22px;
	top: 50%;
	transform: translateY(-50%) rotate(180deg);
	background: url(../images/next_icon.svg) center/cover no-repeat;
	width: 5px;
	height: 9px;
}

/* K-CURE 라이브러리 */
.k_cure_lib_wrap .lib_content{
	background-color: #F9F9F9;
}
.k_cure_lib_wrap .lib_content .link{
	display: grid;
	grid-template-columns: repeat(3,1fr);
	gap: 30px;
	padding-top: 80px;
}
.k_cure_lib_wrap .lib_content .link a{
	border: 1px solid var(--m_c);
	border-radius: 20px;
	padding: 56px 30px;
	color: var(--m_c);
	font-size: 30px;
	font-weight: 700;
	position: relative;
}
.k_cure_lib_wrap .lib_content .link a:after{
	content: '';
	position: absolute;
	right: 30px;
	top: 50%;
	transform: translateY(-50%);
	background: url(../images/plus_icon.svg) center/cover no-repeat;
	width: 16px;
	height: 16px;
}
.k_cure_lib_wrap .lib_content .title{
	font-size: 40px;
	font-weight: 700;
	color: var(--m_c);
	text-align: center;
}

/* 임상 라이브러리 */
.clinical_lib_wrap .lib_content{
	background-color: #F9F9F9;
}
.clinical_lib_wrap .lib_content .title{
	font-size: 26px;
	font-weight: 700;
	color: #111111;
	text-align: center;
	padding-bottom: 32px;
}
.clinical_lib_wrap .lib_content .slide{
	padding: 0 15px;
	overflow: hidden;
}
.clinical_lib_wrap .lib_content .slide .list .slick-track{
	display: flex;
	gap: 20px;
}
.clinical_lib_wrap .lib_content .slide .list .slick-arrow{
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	z-index: 2;
	text-indent: -999999999em;
	border: none;
	background: url(../images/about_clinical_slide_active_btn.svg) center/cover no-repeat;
	width: 30px;
	height: 30px;
}
.clinical_lib_wrap .lib_content .slide .list .slick-prev{
	left: -15px;
	transform: translateY(-50%) rotate(180deg);
}
.clinical_lib_wrap .lib_content .slide .list .slick-prev.slick-disabled{
	background-image: url(../images/about_clinical_slide_disable_btn.svg);
	transform: translateY(-50%) rotate(0deg);
}
.clinical_lib_wrap .lib_content .slide .list .slick-next{
	right: -15px;
}
.clinical_lib_wrap .lib_content .slide .list li{
	border-radius: 10px;
	background-color: #EAEDF8;
	font-size: 20px;
	font-weight: 700;
	color: var(--m_c);
	min-width: 150px;
	text-align: center;
}
.clinical_lib_wrap .lib_content .slide .list li a{
	padding: 18px 10px;
}
.clinical_lib_wrap .lib_content .sub_title{
	font-size: 26px;
	font-weight: 700;
	color: var(--m_c);
	text-align: center;
	padding-top: 55px;
}
.clinical_lib_wrap .lib_content .txt{
	padding-top: 22px;
	font-size: 18px;
	line-height: 1.4;
	text-align: center;
	color: #707070;
}
.clinical_lib_wrap .lib_content .process{
	display: grid;
	grid-template-columns: repeat(3,1fr);
	gap: 30px;
	padding-top: 70px;
}
.clinical_lib_wrap .lib_content .process > div{
	height: 100%;
	padding-top: 25px;
}
.clinical_lib_wrap .lib_content .process .item{
	position: relative;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	border: 1px solid var(--m_c);
	border-radius: 20px;
	height: 100%;
}
.clinical_lib_wrap .lib_content .process .item h5{
	font-size: 18px;
	color: #fff;
	background-color: #02105F;
	padding: 14px 0;
	text-align: center;
	border-radius: 34px;
	position: absolute;
	top: 0;
	transform: translateY(-50%);
	min-width: 200px;
}
.clinical_lib_wrap .lib_content .process .item ul{
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	gap: 10px;
	padding: 0 30px;
}
.clinical_lib_wrap .lib_content .process .item ul li{
	color: var(--m_c);
	font-weight: 500;
	text-align: center;
	padding: 7px 12px;
	border: 1px dotted var(--m_c);
	border-radius: 5px;
}
.clinical_lib_wrap .lib_content .process .left .item ul{
	padding-top: 57px;
	padding-bottom: 27px;
}
.clinical_lib_wrap .lib_content .process .left .item ul li{
	width: calc(33.333% - 10px);
}
.clinical_lib_wrap .lib_content .process .center{
	position: relative;
}
.clinical_lib_wrap .lib_content .process .center:before{
	content: '';
	position: absolute;
	right: 100%;
	top: 50%;
	transform: translateY(-50%);
	background: url(../images/about_clinical_arrow.svg) center/cover no-repeat;
	width: 30px;
	height: 79px;
}
.clinical_lib_wrap .lib_content .process .center:after{
	content: '';
	position: absolute;
	left: 100%;
	top: 50%;
	transform: rotate(180deg) translateY(50%);
	background: url(../images/about_clinical_arrow.svg) center/cover no-repeat;
	width: 30px;
	height: 79px;
}

/* 공공 라이브러리 */

/* .item 공통 style */
.public_lib_wrap .lib_content .con{
	padding-top: 0;
	display: flex;
	flex-direction: column;
	gap: 30px;
}
.public_lib_wrap .lib_content .item{
	border: 1px solid #1E31A0;
	border-radius: 5px;
	overflow: hidden;
	cursor: pointer;
}
.public_lib_wrap .lib_content .item.on{
	border-color: transparent;
	box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}
.public_lib_wrap .lib_content .item.on .tle{
	background-color: var(--m_c);
	color: #fff;
	transition: 0.2s;
}
.public_lib_wrap .lib_content .item.on .tle:after{
	background-image: url(../images/about_public_w_arrow.svg);
	transform: rotate(360deg);
}
.public_lib_wrap .lib_content .tle{
	font-size: 20px;
	font-weight: 700;
	color: var(--m_c);
	padding: 23px 30px;
	position: relative;
}
.public_lib_wrap .lib_content .tle:after{
	content: '';
	position: absolute;
	right: 30px;
	top: 50%;
	transform: translateY(-50%);
	background: url(../images/admin_nav_arrow.svg) center/cover no-repeat;
	width: 11px;
	height: 7px;
	transition: 0.2s;
}
.public_lib_wrap .lib_content .detail{
	padding: 22px 30px;
	display: none;
}
.public_lib_wrap .lib_content .detail .con_title{
	font-size: 18px;
	font-weight: 700;
	color: #111111;
}
.public_lib_wrap .lib_content .detail .list{
	color: #707070;
}


.public_lib_wrap .lib_content .cancel .detail .con_title{
	padding: 30px 0 11px;
	line-height: 1.4;
}
.public_lib_wrap .lib_content .cancel .detail .list ul{
	display: flex;
}
.public_lib_wrap .lib_content .cancel .detail .list ul li{
	flex: 0.34;
	line-height: 1.7;
}
.public_lib_wrap .lib_content .agree .txt{
	display: flex;
	justify-content: space-between;
	gap: 30px;
	padding-top: 32px;
}
.public_lib_wrap .lib_content .agree .txt .list{
	display: flex;
	flex-direction: column;
	gap: 18px;
}
.public_lib_wrap .lib_content .agree .txt .list ul{
	display: flex;
	gap: 10px;
}
.public_lib_wrap .lib_content .agree .txt .list ul li:first-child{
	color: var(--m_c);
	font-weight: 700;
}
.public_lib_wrap .lib_content .agree .txt .img_inn{
	position: relative;
}
.public_lib_wrap .lib_content .agree .txt .img_inn a{
	position: absolute;
	left: 50%;
	top: 50%;
	transform: translate(-50%, -50%);
	color: var(--m_c);
	font-size: 12px;
	font-weight: 700;
	display: flex;
	flex-direction: column;
	align-items: center;
	gap: 8px;
}
.public_lib_wrap .lib_content .schedule .detail .con_title{
	padding-bottom: 30px;
}


