	background-position: center;
	width: 30px;
	height: 30px;
}
.data_file_wrap.on ul:first-child{
	display: none;
}
.data_file_wrap.on ul:last-child{
	display: flex;
}

/* 게시판 상단 탭 */
.board_tab{
	position: relative;
	margin-bottom: 40px;
}
.board_tab:after{
	content: "";
	position: absolute;
	bottom: 0px;
	width: 100%;
	border-bottom: 1px solid #dddddd;
}
.board_tab ul{
	font-size: 0;
}
.board_tab ul li{
	display: inline-block;
	margin-right: -1px;
}

.board_tab ul li a{
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 16px;
	height: 44px;
	padding: 0 30px;
	color: #707070;
	border: 1px solid #DDDDDD;
}
.board_tab ul li a.on,
.board_tab ul li a:hover{
	position: relative;
	border-color: var(--m_c);
	z-index: 1;
	font-weight: 700;
	color: var(--m_c);
}

/* 게시판 상단 역역 */
.board_info{
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-bottom: 10px;
}
.board_info p{
	font-size: 14px;
	color: #707070;
}
.board_info .search{
	display: flex;
	align-items: center;
	grid-gap: 10px;
}
.board_info .search *{
	font-size: 14px;
	height: 30px;
	border-radius: 5px;
}
.board_info .search input,
.board_info .search select{
	border-color: #DDDDDD;
	width: initial;
}
.board_info .search input{
	width: 200px;
}
.board_info .search a{
	display: flex;
	align-items: center;
	justify-content: center;
	color: white;
	background-color: #333333;
	padding: 0 20px;
}
/* 파일첨부 */
.doc_file{
	display: flex;
	grid-gap: 12px 20px;
	flex-wrap: wrap;
}
.doc_file .hwp_file{
	color: #707070;
	display: flex;
	align-items: center;
}
.doc_file .hwp_file:before{
	content: "";
	display: inline-block;
	width: 18px;
	height: 18px;
	background: url('../images/hwp_icon.png')no-repeat;
	background-position: center;
	margin-right: 10px;
}

/* 테이블 진행상태 */
.tb_btn_wrap{
	display: inline-flex;
	align-items: center;
	grid-gap: 10px;
	font-size: 14px;
}
.tb_btn_wrap a{
	display: flex;
	align-items: center;
	justify-content: center;
	height: 30px;
	padding: 0 20px;
	border-radius: 5px;
}

/* 그림자 박스 */
.shadow_box{
	padding: 20px;
	box-shadow: 0px 0px 10px #0000001A;
	border-radius: 10px;
}
.shadow_box ul{
	display: flex;
	flex-direction: column;
	grid-gap: 10px;
	font-size: 18px;
}

/* 화이트 박스 */
.kc_white_box{
	width: 100%;
	border-radius: 10px;
	background-color: white;
	padding: 20px;
}
.sec_white_box{
	padding: 40px 30px;
	border: 1px solid #D6D3D3;
	border-radius: 6px;
}
.sec_white_box img{
	width: 100%;
	display: block;
}
.sec_white_box .list_txt ul{
	display: flex;
	flex-direction: column;
