package kcure.portal.example.service.impl;

import java.io.Serializable;
import javax.persistence.Lob;

import egovframework.com.cop.bbs.service.BoardVO;

@SuppressWarnings("serial")
public class ExEditorVO extends BoardVO implements Serializable{
	
	@Lob
    private  Object  col1;
    private String col2= "";
    private int seq;
    
    
	
    
	public int getSeq() {
		return seq;
	}

	public void setSeq(int seq) {
		this.seq = seq;
	}

	public String getCol2() {
		return col2;
	}

	public void setCol2(String col2) {
		this.col2 = col2;
	}

	public Object getCol1() {
		return col1;
	}

	public void setCol1(Object col1) {
		this.col1 = col1;
	}

	@Override
	public String toString() {
		return "ExEditorVO [col1=" + col1 + ", col2=" + col2 + ", seq=" + seq + "]";
	}

	    
    
   
}
