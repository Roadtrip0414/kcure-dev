@charset "utf-8";

/* layout */
.topnotice-wrap{background:#ddd;}
#wrap{position: relative;width: 100%;}
body.body-noscroll{overflow: hidden !important;}
.inbox-wrap{width: 1600px;max-width:calc(100% - 2rem);margin:0 auto;}

/* header */
#header{width: 100%;z-index: 22;background: #fff;height: 5.6rem;}
#header.header-fixed .header-wrap{position:fixed;z-index: 22;}
.header-wrap{height:5.6rem;width:100%;border-bottom:1px solid #e0e0e0;color:#595757;}
#header.header-on .header-wrap,
#header.header-fixed .header-wrap{background:#fff;color:#595757;border-bottom:1px solid #e0e0e0;}
.header-inbox .logo{position:absolute;top:1rem;left:0;z-index: 3;width: 8.85rem;height: 3.6rem;background: url('../images/img_logo_col.png') 0 0;background-size: 100%;}
.header-inbox .logo a{display: block;width: 100%;height: 100%;}
.header-on .header-inbox .logo,
.header-fixed .header-inbox .logo{background: url('../images/img_logo_col.png') 0 0 !important;background-size: 100% !important;}
.header-inbox{position:relative;width: 1600px;max-width:calc(100% - 2rem);margin:0 auto;}
.header-inbox .top-link01{position: absolute;top:1.1rem;right:0;z-index: 3;font-size: 0.7rem;}
.header-inbox .top-link01::after{display: block;content: '';clear: both;}
.header-inbox .top-link01 li{float: left;margin-left: 1rem;}
.header-inbox .top-link02{position: absolute;top: 2.9rem;right:0;z-index: 3;font-size: 0.8rem;}
.header-inbox .top-link02 .btn-searchopen{display:inline-block;width: 1rem;height: 1rem;margin-left:1.25rem;vertical-align: middle;background: url('../images/img_search_col.png') no-repeat 0 0;background-size: 100%;}
.header-on .header-inbox .top-link02 .btn-searchopen,
.header-fixed .header-inbox .top-link02 .btn-searchopen{background: url('../images/img_search_col.png') no-repeat 0 0 !important;background-size: 100% !important;}
.header-inbox .top-link02 .btn-allmenuopen{display:inline-block;width: 1.1rem;margin-left:1.25rem;vertical-align: middle;}
.header-inbox .top-link02 .btn-allmenuopen .bar{display: block; width: 100%; height: 2px; background:#595757;}
.header-inbox .top-link02 .btn-allmenuopen .bar.num02{ margin: 0.35rem 0;}
.header-on .top-link02 .btn-allmenuopen .bar,
.header-fixed .top-link02 .btn-allmenuopen .bar{background:#595757 !important;}
.sccontrol-box{display: none;}
.sccontrol-box button{width: 18px;}
.sccontrol-box .zoom-plus{margin:0 -1px 0 4px;}
.sccontrol-box .zoom-plus::after{display: inline-block;content: '+';line-height: 1;}
.sccontrol-box .zoom-minus::after{display: inline-block;content: '-';line-height: 1;}
.main-wrap #header{background:none;position: absolute;top:0;left:0;}
.main-wrap .header-wrap{border-bottom:none;color: #fff;}
.main-wrap .header-inbox .logo{background: url('../images/img_logo_wh.png') 0 0;background-size: 100%;}
.main-wrap .header-inbox .top-link02 .btn-searchopen{background: url('../images/img_search_wh.png') no-repeat 0 0 ;background-size: 100%;}
.main-wrap .header-inbox .top-link02 .btn-allmenuopen .bar{background:#fff;}


/* gnb */
#gnb{position: absolute;top:1.7rem;left:0;width:100%;text-align: center;z-index: 2;}
.gnb{display: inline-block;vertical-align: top;}
.gnb::after{display: block;content:'';clear: both;}
.gnb > li{position:relative;float:left;margin:0 0.1rem;padding:0 1.4rem;font-size: 1.1rem;font-weight: 500;}
.gnb > li > span > a{position:relative;display: block;height:3.85rem;line-height:3.85rem;}
.gnb > li.on > span > a,
.gnb > li.current > span > a{color: #2962FF;}
.gnb > li.on > span > a::after{position:absolute;bottom:0.85rem;display: block;content:''; width: 100%;height: 2px;background:#2962FF;}
.gnb > li .submenu-wrap{position:absolute;top:3.85rem;left:50%;transform: translateX(-50%);-webkit-transform: translateX(-50%);padding-top:0.45rem;visibility: hidden; overflow: hidden; height: 0; opacity: 0;text-align: left;white-space: nowrap;/*display: none;*/}
.gnb > li.on .submenu-wrap{visibility: visible; overflow: visible; height: auto;opacity: 1;-webkit-transition:opacity 0.7s, border 0.7s;transition:opacity 0.7s, border 0.7s;}
.gnb .submenu-wrap a:hover{color: #2962FF;}
.gnb .submenu-wrap .munu-depth01{background:#fff;width:9.7rem;border-radius:0.45rem;padding:1.2rem;font-weight: 500;font-size:0.75rem;border: 1px solid #e0e0e0;}
.gnb .submenu-wrap .munu-depth01 li{position:relative;padding-top:0.45rem;}
.gnb .submenu-wrap .munu-depth01 li:first-child{padding-top:0;}
.gnb .submenu-wrap .munu-depth01 li:first-child .submenu-wrap02{top:-1.75rem;padding-top:0.5rem;}
.gnb .submenu-wrap .munu-depth01 > li > a{position:relative;}
.gnb .submenu-wrap .munu-depth01 > li.yesdepth > a::after{display:block;content:'';position:absolute;right:-22px;top:2px;width: 0.95rem;height: 0.95rem;background: url('../images/bul_arrow_blue.png') no-repeat 0 0 ;background-size: 100%;}
.gnb .submenu-wrap .munu-depth01 li.on .submenu-wrap02{display: block;}
.gnb .submenu-wrap .submenu-wrap02{position:absolute;top:0;left:8.4rem;padding-left:0.55rem;display: none;}
.gnb .submenu-wrap .subwrapbot .submenu-wrap02{top:-0.65rem;}
.gnb .submenu-wrap .munu-depth02{min-width:120px;padding:1.2rem;background:#fff;border-radius:0.45rem;font-weight: 400;font-size:0.7rem;border: 1px solid #e0e0e0;}
/*.gnb > li.li-num03 .submenu-wrap .munu-depth01,
.gnb > li.li-num05 .submenu-wrap .munu-depth01{text-align: center;padding-left:0;padding-right:0;}*/
.gnb > li.li-num01 .munu-depth01 li:nth-child(3) .submenu-wrap02{top:-0.75rem;}
.gnb > li.li-num01 .munu-depth01 li:nth-child(4) .submenu-wrap02{top:-2.15rem;}
.gnb > li.li-num01 .munu-depth01 li:nth-child(5) .submenu-wrap02{top:-0.7rem;}
.gnb > li.li-num04 .munu-depth01 li:nth-child(5) .submenu-wrap02{top:-2.15rem;}
.gnb > li.li-num04 .munu-depth01 li:nth-child(6) .submenu-wrap02{top:-2.25rem;}


/* containter */
#container{padding:0 0 4rem;min-height:calc(100vh - 28.4rem);background: #fcfcfc;}
.main-wrap #container{padding:0;min-height:0;background:none;}
.subtop-tab{white-space: nowrap;background: #0D3CBE;color:#A5BDFF;font-size: 14px;font-weight: 500;}
.subtop-tab li{display: inline-block;}
.subtop-tab li a{position:relative;display: block;padding:0.475rem 0;margin:0 0.55rem}
.subtop-tab li:first-child a{margin-left: 0;}
.subtop-tab li a:hover,
.subtop-tab li.on a{color: #fff;}
.subtop-tab li a:hover::after,
.subtop-tab li.on a::after{position:absolute;bottom:0;left:0;display: block;content: '';width: 100%;height: 1px;background: #fff;}
.subtop-tab li a span{display: block;}
.subtop-infor{position:relative;padding:1.2rem 0 1.45rem;background: #2962FF;color: #fff;}
.subtop-infor .txt-infor{word-break: keep-all;font-size: 14px;line-height: 20px;}
.subtop-infor .inbox-wrap{position: relative;}
.subtop-infor .location{display:inline-block;position:absolute;top:0;right: 0;padding-right:30px;font-size:13px;color: #b2c7fe;}
.subtop-infor .location a.home{display: inline-block;width: 17px;height: 14px;vertical-align:middle;margin-top:-2px;background: url('../images/ico_home.png') no-repeat 0 0 ;background-size: 100%;}
.subtop-infor .location a span{vertical-align: middle;padding:0 6px 0 12px;}
.subtop-infor .location a::before{display:inline-block;content:'';padding: 2px;border: solid #c8d7ff;border-width: 1px 1px 0 0;vertical-align:middle;transform: rotate(45deg);-webkit-transform: rotate(45deg);}
.subtop-infor .location a:first-child::before{display: none;}
.subtop-infor .location a.current span{color: #fff;padding-right:0;}
.subtop-infor .location a.current{cursor: default;pointer-events: none;}
.subtop-infor .location .locamenu-box{position:absolute;display: inline-block;z-index: 1;}
.subtop-infor .location a.lt-openmenu{position:relative;display: inline-block;width: 15px;height: 14px;margin-left: 8px;vertical-align: middle;}
.subtop-infor .location a.lt-openmenu::after{display: block;content: ''; width: 100%;height: 100%;background:#c8d7ff;border-radius: 100%;}
.subtop-infor .location a.lt-openmenu::before{display:inline-block;content:'';position: absolute;padding:2px;top:4px;left:5px;border-color:#326aff;transform: rotate(135deg);-webkit-transform: rotate(135deg);}
.subtop-infor .location a.lt-openmenu.on::before{top:6px;transform: rotate(-45deg);-webkit-transform: rotate(-45deg);}
.subtop-infor .location-menu{display:none;position: absolute;right:0;width:120px;padding:10px;overflow: hidden;border: 1px solid #e0e0e0;border-radius:10px;font-size:0.7rem;color: #595757;opacity: 0; background: #fff;transition:all 0.5s;-webkit-transition:all 0.5s;}
.subtop-infor .location.open .location-menu{display:block;opacity: 1;}
.subtop-infor .list-type02{margin-left:9px;}
.subtop-infor .list-type02 li{color:#fff;font-size: 14px;line-height: 20px;}
.subtop-infor .list-type02 li::before{background-color: #fff;width: 4px;height: 4px;}
.subtop-infor h2{padding-bottom: 0.3rem;font-size: 0.9rem;font-weight: 500;}
.subtop-infor .link{text-decoration: underline;}
.subcon-wrap .inbox-wrap{padding-top: 1.5rem;}


/* footer */
#footer{width: 100%;padding:3rem 0;background:#010e31;color:#bfc3cb;}
.footer-wrap{position:relative;width:1600px;max-width:calc(100% - 2rem);margin:0 auto;}
.footer-wrap .footlogo{float: left;}
.footer-wrap .footlogo a{display: block;width: 8.9rem;height: 3.6rem;background: url('../images/img_logo_footer.png') no-repeat 0 0 ;background-size: 100%;}
.footer-wrap address{padding-top: 2.8rem;}
.footer-link::after{display: block;content: '';clear: both;}
.footer-link .link-ul{float: left;padding:2.15rem 0 0 3rem;font-size: 0.9rem;line-height: 1.1rem;}
.footer-link .link-ul a:hover{text-decoration: underline;}
.footer-link .link-ul li{float: left;padding-left: 1.5rem;}
.footer-link .link-ul li:first-child{padding-left: 0;}
.footer-link .link-ul:after{display: block;content: '';clear: both;}
.footer-site{position: absolute;top:1.8rem;right:0;}
.footer-site .btn-sopen{position:relative;display:block;width:200px;height:36px;padding:0 0 0 20px;border: 1px solid #4D566F;border-radius: 3.5px;color:#808597;text-align: left;}
.footer-site .btn-sopen:before{display:inline-block;content:'';position:absolute;top:9px;right:20px;border: solid #4D566F;border-width: 0 1px 1px 0;padding: 5px;transform: rotate(45deg);-webkit-transform: rotate(45deg);transition: all 0.2s;}
.footer-site .btn-sopen.on:before{top:16px;transform: rotate(-135deg);-webkit-transform: rotate(-135deg);}
.footer-site .linklist-box{display:none;position:absolute;top:36px;width:100%;background:#000;padding:10px;}
.footer-site.open .linklist-box{display:block;}
.btn-wintop{display:none;position:fixed;left:50%;bottom:7.9rem;margin-left:40.75rem;height: 36px;width: 36px;background:#2962ff;}
.btn-wintop::after{position:absolute;top:50%;left:50%;display: block;content: '';width: 12px;height: 13px;transform: translate(-50%, -50%);-webkit-transform: translate(-50%, -50%);background: url('../images/ico_topbtn.png') no-repeat 0 0;background-size: 100%;}

@media screen and (max-width: 1400px) {
    html{font-size: 19px;}
    .gnb > li{padding: 0 1rem;}
}
@media screen and (max-width: 1200px) {
    #gnb, #gnb-bg{display: none;}
    .header-wrap{border-bottom:1px solid #dcdddd;}
    .sccontrol-box{display: none;}
    .subtop-infor .txt-infor{width: 80%;}
}
@media screen and (max-width: 820px) {
    html{font-size: 18px;}

    .footer-site{position: relative;}
    .footer-site .btn-sopen{width: 100%;}
}
@media screen and (max-width: 819px) {
    html{font-size: 16px;}

    .subtop-infor .location{position: relative;padding-bottom: 1rem;}
    
    .footer-site{position: relative;}
    .footer-site .btn-sopen{width: 100%;}
}