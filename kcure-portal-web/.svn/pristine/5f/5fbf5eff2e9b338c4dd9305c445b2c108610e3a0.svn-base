/* 데이터셋 */
.data_request_wrap{
	display: flex;
	flex-direction: column;
	gap: 60px;
}
.data_request_wrap .data_provision .list{
	display: flex;
	flex-direction: column;
	gap: 10px;
	background-color: #F9F9F9;
	padding: 20px;
}
.data_request_wrap .detail_set{
	display: flex;
	grid-gap: 18px;
}
.data_request_wrap .detail_set .left{
	flex: 1;
}
.data_request_wrap .detail_set .left .variable{
	background-color: #F9F9F9;
	height: 550px;
	padding: 30px 20px;
	display: flex;
	flex-direction: column;
	gap: 20px;
	overflow-y: auto;
}
.data_request_wrap .detail_set .left .variable .list{
	color: #707070;
}
.data_request_wrap .detail_set .left .variable .list .depth_01{
	color: #111111;
	font-weight: 700;
}
.data_request_wrap .detail_set .left .variable .list .sub_depth{
	display: flex;
	flex-direction: column;
	gap: 10px;
	padding-top: 10px;
	padding-left: 10px;
}
.data_request_wrap .detail_set .left .variable .list .sub_depth a.on{
	color: var(--m_c);
	font-weight: 700;
}
.data_request_wrap .detail_set .right{
	width: 880px;
}
.data_request_wrap .detail_set .item{
	background-color: #F9F9F9;
	padding: 30px 20px;
	height: 550px;
	overflow-y: auto;
}
.data_request_wrap .detail_set .right .item.clinical{
	display: none;
}
.data_request_wrap .detail_set .right .item.clinical .list{
	width: fit-content;
}
.data_request_wrap .detail_set .right.on .item.public{
	display: none;
}
.data_request_wrap .detail_set .right.on .item.clinical{
	display: block;
}
.data_request_wrap .detail_set .right .item.public .list{
	display: inline-flex;
	flex-direction: column;
	gap: 20px;
}
.data_request_wrap .detail_set .right .list ul li{
	font-weight: 400;
}
.data_request_wrap .detail_set .right .list ul ul{
	display: none;
	padding-left: 18px;
}
.data_request_wrap .detail_set .right .list a{
	padding-left: 18px;
	margin-bottom: 20px;
	display: flex;
	gap: 10px;
}
.data_request_wrap .detail_set .right .list ul li a{
	position: relative;
}
.data_request_wrap .detail_set .right .list > ul > li > a > label{
	position: absolute;
	z-index: 10;
}
.data_request_wrap .detail_set .right .list > ul > li > a > .txt{
	padding-left: 32px;
}
.data_request_wrap .detail_set .right .list ul li a:not(:last-child):after{
	content: "";
	position: absolute;
	left: -2px;
	top: 50%;
	transform: translateY(-50%);
	width: 13px;
	height: 13px;
	background: url('../images/drop_close_icon.svg') no-repeat;
	background-position: center;
	transition: all 0.3s;
}
.data_request_wrap .detail_set .right .list ul li a.on:after{
	background-image: url('../images/drop_open_icon.svg');
	transform: translateY(-50%) rotate(90deg);
}
.data_request_wrap .detail_set .right .list label.n_checkbox_s input[type="checkbox"]:checked + span{
	font-weight: 700;
}
.data_request_wrap .detail_set .right .list ul li a.disable{
	pointer-events: none;
}
.data_request_wrap .detail_set .right .list ul li a.disable span{
	color: #AAAAAA;
}
.data_request_wrap .detail_set .right .list ul li a.disable label.n_checkbox_s .check_img{
	background-image: none;
	background-color: #DDDDDD;
}
.data_request_wrap .detail_set .right .list ul li a .txt.on{
	font-weight: 700;
}
.data_request_wrap .data_unite .item{
	border-top: 1px solid var(--l_d);
