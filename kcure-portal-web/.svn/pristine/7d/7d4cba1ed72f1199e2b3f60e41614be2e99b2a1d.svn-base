}
.admin input[type="radio"]:checked + span.radio_mark:before {
}
.admin input[type="radio"]:checked + span.radio_mark:after {
	content: '';
	position: absolute;
	top: calc(50% + 0.5px);
	transform: translateY(-50%);
	left: 6px;
	width: 10px;
	height: 10px;
	background: #0F249A;
	border-radius: 100%;
}

