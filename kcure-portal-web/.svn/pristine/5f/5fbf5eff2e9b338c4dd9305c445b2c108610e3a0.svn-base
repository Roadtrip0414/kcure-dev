	min-width: 155px;
	width: 100%;
}
.re_layer_popup .re_popup_box .popup_con .pop_tbl_inn .n_table_info .n_l_table .data_file_wrap ul label .btn_upload, 
.re_layer_popup .re_popup_box .popup_con .pop_tbl_inn .n_table_info .n_l_table .data_file_wrap ul li .del_btn{
	width: 75px;
	height: 26px;
}
.re_layer_popup .re_popup_box .popup_con .pop_tbl_inn .n_table_info .n_l_table .data_file_wrap ul:last-child li:first-child:before{
	width: 20px;
	height: 20px;
	background-size: cover;
}
.re_layer_popup .re_popup_box .popup_con .use_center_info{
	padding: 0;
	border: none;
}
.re_layer_popup .re_popup_box .popup_con .explan{
	padding: 20px;
	background-color: #f7f7f7;
	border: 1px solid #D6D3D3;
	border-radius: 6px;
}
.re_layer_popup .re_popup_box .popup_con .explan span{
	color: #FF2222;
	font-weight: 700;
}
.re_layer_popup .re_popup_box .popup_con .top{
	flex-direction: column;
	grid-gap: 0;
}
.re_layer_popup .re_popup_box .pop_btn{
	display: flex;
	grid-gap: 15px;
	padding: 0 35px 30px;
}
.re_layer_popup .re_popup_box .pop_btn a{
	flex-grow: 1;
}
.re_layer_popup .re_popup_box .pop_btn a:nth-child(2){
	background-color: #444444;
	color: white !important;
}
.re_layer_popup .re_popup_box .pop_btn a:nth-child(2):hover{
	background-color: black !important;
}
.ad_re_r_table{
	margin-bottom: 25px;
}
.ad_re_r_table table{
	width: 100%;
	border: none;
	border-spacing: 0;
	border-collapse: collapse;
	font-size: inherit;
	color: inherit;
	border-bottom: 1px solid #E5E5E5;
}
.ad_re_r_table table thead th,
.ad_re_r_table table thead td{
	border-top: 1px solid #444444;
}
.ad_re_r_table table th,
.ad_re_r_table table td{
	border: none;
	border-spacing: 0;
	border-collapse: collapse;
	font-size: inherit;
	color: inherit;
	text-align: left;
	height: 52px;
	line-height: 1.4;
	word-break: keep-all;
	border: 1px solid #E5E5E5;
	font-size: 16px;
	text-align: center;
}
.ad_re_r_table table th{
	font-weight: 500;
	color: #0D0D0D;
	background-color: #F4F4F4;
	padding: 0px 30px;
}
.ad_re_r_table table th:first-child{
	border-left: none;
}
.ad_re_r_table table th:last-child{
	border-right: none;
}
.ad_re_r_table table td{
	padding: 6px 14px;
}
.ad_re_r_table table td:first-child{
	border-left: none;
}
.ad_re_r_table table td:last-child{
	border-right: none;
}
