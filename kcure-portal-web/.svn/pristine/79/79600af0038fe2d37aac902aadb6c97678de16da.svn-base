<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ExampleFile">

	<select id="selectList"  resultType="hashmap">
		select atch_file_id,count(*) cnt from COMTNFILEDETAIL
		where stre_file_nm like 'SAMPLE_%'
		group by atch_file_id
		order by atch_file_id desc
	</select>
	
</mapper>