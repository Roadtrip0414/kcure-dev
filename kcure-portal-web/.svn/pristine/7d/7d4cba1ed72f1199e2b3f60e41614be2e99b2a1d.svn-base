@charset "UTF-8";
@import url('team.css');


/* 메인 */
.main_view_wrap{
	background: url('../images/main_img.svg')no-repeat;
	background-position: top center;
	background-size: cover;
	padding-top: 120px;
	text-align: center;
}
.main_view_wrap .sub_title{
	font-size: 30px;
	font-weight: 300;
	color: #02105F;
	margin-bottom: 24px;
}
.main_view_wrap .main_title{
	font-size: 80px;
	font-weight: 700;
	color: #02105F;
	margin-bottom: 36px;
}
.main_view_wrap .main_search{
	position: relative;
	margin-bottom: 26px;
}
.main_view_wrap .main_search input{
	height: 70px;
	border: 2px solid #02105F;
	border-radius: 35px;
	padding: 0px 85px 0px 30px;
}
.main_view_wrap .main_search a{
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 30px;
	width: 25px;
	height: 25px;
	background: url('../images/m_search_icon.svg')no-repeat;
	background-position: center;
	background-size: contain;
}
.main_view_wrap .keyword{
	display: flex;
	grid-gap: 20px;
	align-items: center;
	justify-content: center;
	font-size: 20px;
	font-weight: 500;
	color: #02105F;
	margin-bottom: 58px;
}
.main_view_wrap .quick_menu{
	display: inline-flex;
	grid-gap: 36px;
	margin-bottom: 90px;
}
.main_view_wrap .quick_menu a{
	width: 128px;
	font-weight: 500;
	color: #02105F;
}
.main_view_wrap .quick_menu a ul{
	display: flex;
	flex-direction: column;
	grid-gap: 18px;
	align-items: center;
}
.main_view_wrap .quick_menu a ul li{
	line-height: 1.3;
}
.main_view_wrap .quick_menu a ul li:first-child{
	display: flex;
	align-items: center;
	justify-content: center;
	width: 95px;
	height: 95px;
	border-radius: 15px;
	box-shadow: 0px 0px 16px rgb(0 0 0 / 16%);
	background-color: white;
}
.main_view_wrap .data_link{
	display: grid;
	grid-template-columns: 1fr 1fr 1fr 1fr;
	grid-gap: 20px;
	margin-bottom: 20px;
}
.main_view_wrap .data_link a{
	position: relative;
	padding: 20px;
	height: 118px;
	border: 1px solid #0F249A;
	border-radius: 10px;
	display: flex;
	align-items: center;
}
.main_view_wrap .data_link a:after{
	content: "";
	position: absolute;
	right: 20px;
	bottom: 32px;
	width: 15px;
	height: 15px;
	background: url('../images/plus_icon.svg')no-repeat;
}
.main_view_wrap .data_link a ul{
	display: flex;
	flex-direction: column;
	grid-gap: 16px;
	font-size: 20px;
	font-weight: bold;
	color: #0F249A;
	text-align: left;
}
.main_view_wrap .data_link a ul li:last-child span{
	font-weight: normal;
}
.main_view_wrap .data_update{
	display: flex;
	align-items: center;
	justify-content: flex-end;
	font-size: 14px;
	color: #0F249A;
}
.latest_wrap .con{
	display: flex;
	grid-gap: 30px;
}
.latest_wrap .data{
	flex-grow: 1;
}
.latest_wrap .data .title{
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 40px;
	font-weight: 700;
	color: #111111;
	margin-bottom: 20px;
}
.latest_wrap .data .list{
	display: flex;
	flex-direction: column;
	justify-content: center;
	background-color: white;
	border: 1px solid #DDDDDD;
	border-radius: 10px;
	height: 302px;
	padding: 0 30px;
}
.latest_wrap .data .list a{
	align-items: center;
	flex-grow: 1;
	white-space: normal;
	display: -webkit-box;
	-webkit-line-clamp: 1;
	-webkit-box-orient: vertical;
	overflow: hidden;
}
.latest_wrap .data .list a span{
	display: inline-block;
	font-size: 14px;
	padding: 0 10px;
	border-radius: 5px;
	height: 30px;
	line-height: 29px;
	margin-right: 10px;
}
.latest_wrap .data .list a .notice{
	background-color: #F4F4F4;
}
.latest_wrap .data .list a .news{
	background-color: #EAEDF8;
}
.latest_wrap .data .list p{
	white-space: nowrap;
	color: #707070;
}
.latest_wrap .data .list .item{
	display: flex;
	grid-gap: 30px;
	align-items: center;
	justify-content: space-between;
	font-size: 18px;
	height: 70px;
	line-height: 1.4;
	border-bottom: 1px solid #DDDDDD;
}
.latest_wrap .data .list .item:last-child{
	border: none;
}
.latest_wrap .slide{
	width: 400px;
}
.latest_wrap .slide .top{
	display: flex;
	align-items: flex-end;
	justify-content: space-between;
	margin-bottom: 20px;
}
.latest_wrap .slide .top h2{
	font-size: 40px;
	font-weight: 700;
}
.latest_wrap .slide .top ul{
	display: flex;
	align-items: center;
	justify-content: flex-end;
}
.latest_wrap .slide .top ul li{
	font-family: 'Montserrat', sans-serif;
	font-size: 20px;
	color: #0F249A;
}
.latest_wrap .slide .top ul li:first-child{
	font-weight: bold;
}
.latest_wrap .slide .top ul li:not(:last-child){
	padding-left: 10px;
}
.latest_wrap .slide .top ul .push_btn a{
	width: 30px;
	height: 30px;
	border-radius: 50%;
	margin-left: 10px;
	background: url('../images/push_icon.svg')no-repeat;
	background-position: center;
	background-color: #0F249A;
}
.latest_wrap .slide .top ul .push_btn.on a{
	background-image: url('../images/play_icon.svg');
	background-size: 11px;
}
.latest_wrap .slide .item{
	border-radius: 10px;
	overflow: hidden;
}
.latest_wrap .slide .item .list{
	border-radius: 10px;
}
.latest_wrap .slide .item a{
	border-radius: 10px;
	border: 1px solid transparent;
}
.latest_wrap .slide .item a:hover{
	border: 1px solid black;
}
.latest_wrap .slide .item a img{
	width: 100%;
	height: 300px;
}
.latest_wrap .slide .item a ul{
	display: flex;
	flex-direction: column;
	justify-content: center;
	grid-gap: 26px;
	height: 100%;
	line-height: 1.3;
}
.latest_wrap .slide .item a ul li:first-child{
	font-size: 30px;
	font-weight: bold;
}
.latest_wrap .slide .item button{
	position: absolute;
	bottom: 34px;
	font-size: 0;
	border: none;
	background-color: transparent;
	z-index: 10;
	cursor: pointer;
}
.latest_wrap .slide .item button:after{
	content: '';
	position: absolute;
	width: 6px;
	height: 12px;
	background: url('../images/slide_prev_icon.svg')no-repeat;
	background-position: center;
	background-size: cover;
}
.latest_wrap .slide .item button.slick-prev{
	right: 20px;
}
.latest_wrap .slide .item button.slick-next{
	right: 46px;
}
.latest_wrap .slide .item button.slick-next:after{
	transform: rotate(180deg);
}
.request_wrap .con{
	display: grid;
	grid-template-columns: 1fr 1fr 1fr;
	grid-gap: 30px;
	padding-top: 0px;
}
.request_wrap a{
	position: relative;
	width: 100%;
	height: 186px;
	padding: 38px 26px;
	background-color: white;
	box-shadow: 0px 0px 16px rgb(0 0 0 / 10%);
	border-radius: 10px;
}
.request_wrap a p{
	font-size: 24px;
	font-weight: 700;
	color: #0F249A;
	margin-bottom: 14px;
}
.request_wrap a span{
	line-height: 1.3;
}
.request_wrap a:after{
	background-size: 72px;
	background-position: right bottom;
	background-repeat: no-repeat;
}
.request_wrap a:nth-child(1):after{
	content: "";
	position: absolute;
	right: 26px;
	bottom: 26px;
	width: 80px;
	height: 80px;
	background-image: url('../images/request_icon_01.svg');
}
.request_wrap a:nth-child(2):after{
	content: "";
	position: absolute;
	right: 26px;
	bottom: 26px;
	width: 80px;
	height: 80px;
	background-image: url('../images/request_icon_02.svg');
}
.request_wrap a:nth-child(3):after{
	content: "";
	position: absolute;
	right: 26px;
	bottom: 26px;
	width: 80px;
	height: 80px;
	background-image: url('../images/request_icon_03.svg');
}

/* 로그인 */
.login_wrap{
	width: 400px;
	margin: 0 auto;
	text-align: right;
	padding: 200px 0px;
}
.login_wrap h1{
	text-align: center;
	font-size: 30px;
	font-weight: 700;
	color: var(--m_c);
	margin-bottom: 56px;
}
.login_wrap .int{
	display: flex;
	flex-direction: column;
	grid-gap: 20px;
	margin-bottom: 34px;
}
.login_wrap .pass_lost{
	display: inline-flex;
	justify-content: flex-end;
	font-size: 14px;
	color: #707070;
	margin-bottom: 10px;
}
.login_wrap .login_btn{
	display: flex;
	align-items: center;
	justify-content: center;
	height: 50px;
	border-radius: 10px;
	padding: 0 20px;
	font-size: 18px;
	font-weight: 700;
	color: white;
	background-color: var(--m_c);
}
.login_wrap .login_btn:before{
	content: "";
	background-position: center;
	background-repeat: no-repeat;
	background-size: contain;
	margin-right: 10px;
}
.login_wrap .join{
	display: flex;
	align-items: center;
	justify-content: center;
	padding-top: 26px;
	padding-bottom: 62px;
}
.login_wrap .join a{
	font-size: 16px;
	font-weight: 700;
	margin-left: 30px;
}
.login_wrap .social_login{
	display: flex;
	flex-direction: column;
	grid-gap: 10px;
}
.login_wrap .login_btn.one{
	background-color: #1A2674;
}
.login_wrap .login_btn.one:before{
	width: 22px;
	height: 22px;
	background-image: url('../images/login_icon_01.png');
}
.login_wrap .login_btn.kakao{
	background-color: #FFD800;
	color: #111111;
}
.login_wrap .login_btn.kakao:before{
	width: 22px;
	height: 22px;
	background-image: url('../images/login_icon_02.png');
}
.login_wrap .login_btn.naver{
	background-color: #06BD34;
}
.login_wrap .login_btn.naver:before{
	width: 22px;
	height: 22px;
	background-image: url('../images/login_icon_03.png');
}
.login_wrap .login_btn.medical{
	background-color: #05BBC9;
}
.login_wrap .login_btn.medical:before{
	width: 22px;
	height: 22px;
	background-image: url('../images/login_icon_04.svg');
}

/* 비밀번호 찾기 */
.pass_lost_wrap{
	display: flex;
	align-items: center;
	justify-content: center;
	padding: 200px 0px;
}
.pass_lost_wrap .info{
	text-align: center;
	width: 600px;
}
.pass_lost_wrap .title{
	margin-bottom: 40px;
}
.pass_lost_wrap .title h1{
	font-size: 30px;
	font-weight: 700;
	color: var(--m_c);
	margin-bottom: 26px;
}
.pass_lost_wrap .title .sub{
	font-size: 18px;
	line-height: 1.4;
}
.pass_lost_wrap .panel_item{
	display: flex;
	flex-direction: column;
	grid-gap: 30px;
	margin-bottom: 42px;
}
.pass_lost_wrap .panel_item ul{
	position: relative;
}
.pass_lost_wrap .panel_item .see{
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 20px;
	background: url('../images/see_icon.svg')no-repeat;
	background-position: center;
	width: 18px;
	height: 14px;
}
.pass_lost_wrap .panel_item input{
	height: 50px;
	border-radius: 5px;
	padding: 0 60px 0px 20px;
}
.pass_lost_wrap .panel_item input:focus{
	border: 1px solid var(--m_c);
	outline: none;
}
.pass_lost_wrap .panel_item .wrong{
	font-size: 14px;
	color: #FF2929;
	height: 17px;
	text-align: left;
	margin-top: 10px;
}
.pass_lost_wrap .summit_btn{
	display: inline-flex;
	align-items: center;
	justify-content: center;
	min-width: 250px;
	height: 60px;
	border-radius: 10px;
	background-color: var(--m_c);
	font-size: 20px;
	font-weight: 700;
	color: white;
}
.pass_lost_wrap .bg .info{
	width: 1200px;
	background-color: #F4F4F4;
	padding: 68px 0;
	border-radius: 10px;
	margin-bottom: 16px;
}
.pass_lost_wrap .bg .inquiry{
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 14px;
	color: #707070;
}
.pass_lost_wrap .bg .inquiry a{
	font-weight: 700;
	color: var(--m_c);
	margin-left: 4px;
}
.pass_lost_wrap .panel_item .see.on{
	background-image: url(../images/see_icon_02.svg);
}

/* 데이터 신청 */
/* 개요 신청방법 */
.sub_visual_wrap .data_request{
	/*	background-image: url('../images/data_request_img.png');*/
}
.data_explan_wrap .sub_pag_title{
	grid-gap: 20px;
}
.data_explan_wrap .sub_pag_title p{
	color: #111111;
}
.data_overview_wrap{
	background-color: #F9F9F9;
}
.data_overview_wrap .line_btn{
	display: flex;
	align-items: center;
	justify-content: center;
	margin-bottom: 68px;
}
.data_overview_wrap .line_btn a{
	width: 150px;
	text-align: center;
	padding-bottom: 14px;
	font-size: 20px;
	color: #707070;
	border-bottom: 2px solid #DDDDDD;
}
.data_overview_wrap .line_btn a:hover,
.data_overview_wrap .line_btn a.on{
	font-weight: 700;
	color: var(--m_c);
	border-color: var(--m_c);
}
.data_overview_wrap .nav_btn{
	display: flex;
	align-items: center;
	justify-content: center;
	grid-gap: 20px;
	margin-bottom: 60px;
}
.data_overview_wrap .nav_btn a{
	display: flex;
	align-items: center;
	justify-content: center;
	padding: 0 20px;
	width: 138px;
	height: 60px;
	font-size: 20px;
	font-weight: 700;
	color: var(--m_c);
	background-color: #EAEDF8;
	border-radius: 10px;
}
.data_overview_wrap .nav_btn a.on{
	background-color: var(--m_c);
	color: white;
}
.data_overview_wrap .section{
	display: flex;
	flex-direction: column;
	grid-gap: 100px;
}
.data_overview_wrap .section .item{
	display: flex;
	flex-direction: column;
	grid-gap: 20px;
}
.data_overview_wrap .section .title{
	display: flex;
	flex-direction: column;
	grid-gap: 10px;
}
.data_overview_wrap .section .title h3{
	font-size: 18px;
	font-weight: 700;
}
.data_overview_wrap .section .title ul{
	color: #707070;
	line-height: 1.4;
}
.data_overview_wrap .section table{
	width: 100%;
	border: none;
	border-spacing: 0;
	border-collapse: collapse;
	font-size: inherit;
	color: inherit;
}
.data_overview_wrap .section table tr{
	font-size: inherit;
	color: inherit;
	border: none;
	border-spacing: 0;
	border-collapse: collapse;
}
.data_overview_wrap .section table th,
.data_overview_wrap .section table td{
	border: none;
	border-spacing: 0;
	border-collapse: collapse;
	font-size: inherit;
	color: inherit;
	text-align: center;
	height: 54px;
	padding: 14px 30px;
	line-height: 1.4;
	word-break: keep-all;
	border-top: 1px solid #AAAAAA;
	border-bottom: 1px solid #AAAAAA;
	white-space: nowrap;
}
.data_overview_wrap .section table th:not(:first-child),
.data_overview_wrap .section table td:not(:first-child){
	border-left: 1px solid #AAAAAA;
}
.data_overview_wrap .section table th{
	font-weight: 700;
	vertical-align: text-top;
	background-color: #EAEDF8;
}
.data_overview_wrap .section table td:first-child{
	font-weight: 700;
}
.data_overview_wrap .section table td:not(:nth-child(3)){
	text-align: left;
}
.data_overview_wrap .section table td a{
	color: var(--m_c);
	text-decoration: underline;
	text-underline-position: under;
}

/* 신청하기 */
.data_apply_wrap .apply_list{
	display: grid;
	grid-template-columns: repeat(auto-fit, minmax(10px, 1fr));
	grid-gap: 30px;
	font-size: 18px;
	font-weight: 700;
	text-align: center;
	padding-bottom: 58px;
}
.data_apply_wrap .apply_list ul{
	display: flex;
	flex-direction: column;
	align-items: center;
	grid-gap: 16px;
	line-height: 1.4;
}
.data_apply_wrap .apply_list ul li img{
	width: 140px;
	display: block;
}
.data_apply_wrap .apply_list ul .btn a{
	width: fit-content;
	min-width: 74px;
	height: 30px;
	line-height: 29px;
	padding: 0 12px;
	font-size: 14px;
	color: white;
	background-color: #333333;
	border-radius: 5px;
}
.data_apply_wrap .provision{
	display: flex;
	flex-direction: column;
	grid-gap: 44px;
}
.data_apply_wrap .provision .txt li:first-child{
	font-size: 18px;
	font-weight: 500;
	margin-bottom: 16px;
}
.data_apply_wrap .provision .txt li:last-child{
	font-size: 14px;
	color: #707070;
}
.data_apply_wrap .provision .info{
	padding: 16px;
	height: 100px;
	border-radius: 10px;
	border: 1px solid #DDDDDD;
	margin-top: 20px;
	margin-bottom: 12px;
	overflow-y: auto;
}
.data_apply_wrap .provision .check_btn{
	display: flex;
	align-items: center;
	justify-content: flex-end;
}

/* 요청정보보기 */
.data_view_wrap, .data_view_wrap .info{
	display: flex;
	flex-direction: column;
	grid-gap: 60px;
}
.data_view_wrap .list{
	padding: 14px 0px;
	border-top: 1px solid #AAAAAA;
	border-bottom: 1px solid #AAAAAA;
}
.data_view_wrap .info .btn,
.data_view_wrap .request .btn{
	display: flex;
	align-items: center;
	justify-content: flex-end;
	margin-top: 20px;
}
.data_view_wrap .info .btn a,
.data_view_wrap .request .btn a{
	display: flex;
	align-items: center;
	justify-content: center;
	min-width: 200px;
	height: 50px;
	line-height: 49px;
	padding: 0 20px;
	border-radius: 5px;
	background-color: #333333;
	text-align: center;
	color: white;
	font-weight: 700;
}
.data_view_wrap .info .btn a:before,
.data_view_wrap .request .btn a:before{
	content: "";
	display: flex;
	background: url('../images/tb_plus_icon.svg')no-repeat;
	background-size: 14px;
	background-position: center;
	width: 10px;
	height: 10px;
	margin-right: 10px;
}
.data_finish_wrap{
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	grid-gap: 28px;
	padding: 60px;
	border: 1px solid #DDDDDD;
	border-radius: 10px;
}
.data_finish_wrap img{
	width: 130px;
}
.data_finish_wrap .txt{
	text-align: center;
	color: #707070;
}
.data_finish_wrap .txt span{
	display: inline-block;
	font-size: 18px;
	font-weight: 700;
	margin-bottom: 12px;
	color: #111111;
}

/* 데이터 신청 임상데이터 신청 */
.n_data_flow_wrap{
	position: relative;
	display: flex;
	justify-content: space-between;
	border: 1px solid #C7C7C7;
	border-radius: 8px;
	padding: 36px 50px 26px 50px;
}
.n_data_flow_wrap:before{
	content: "";
	position: absolute;
	top: 45px;
	left: 50%;
	transform: translateX(-50%);
	width: calc(100% - 210px);
	border-bottom: 1px solid #C0D2DC;
}
.n_data_flow_wrap ul{
	display: flex;
	flex-direction: column;
	align-items: center;
	grid-gap: 20px;
	width: 92px;
	color: #848484;
	font-size: 14px;
	font-weight: 500;
	line-height: 1.4;
	z-index: 1;
}
.n_data_flow_wrap ul{
	white-space: nowrap;
}
.n_data_flow_wrap ul .icon{
	width: 20px;
	height: 20px;
	background: url('../images/data_flow_icon_off.svg')no-repeat;
	background-position: center;
	background-size: cover;
}
.n_data_flow_wrap ul.on{
	color: var(--m_c);
}
.n_data_flow_wrap ul.on .icon{
	background-image: url('../images/data_flow_icon_on.svg');
}
.n_data_apply_wrap .personal_info{
	font-size: 12px;
	color: #848484;
	margin-top: 36px;
}
.n_data_apply_wrap .personal_info h3{
	font-size: 18px;
	font-weight: 500;
	color: #111111;
	margin-bottom: 12px;
}
.n_data_apply_wrap .personal_info .description{
	padding: 16px 22px;
	background-color: #F7F7F7;
	border: 1px solid #C0C0C0;
	border-radius: 4px;
	height: 100px;
	line-height: 1.4;
	overflow-y: auto;
	margin-bottom: 16px;
}
.n_data_apply_wrap .personal_info .re_checkbox_s{
